<xml xmlns="http://www.w3.org/1999/xhtml" id="toolbox" style="display: none;">
  <category name="Node-RED" colour="#BB8FCE">
    <block type="node_send">
      <value name="OUT_NR_INPUT">
        <shadow type="math_number">
          <field name="NUM">1</field>
        </shadow>
      </value>
      <value name="MESSAGE_INPUT">
        <shadow type="node_msg"></shadow>
      </value>       
    </block>
    <block type="node_msg"></block>
    <block type="node_flow_memory"></block>
    <block type="node_global_memory"></block>
    <block type="node_context_memory"></block>
    <block type="node_return">
      <value name="MESSAGE_INPUT">
        <shadow type="node_msg"></shadow>
      </value>
    </block> 
    <block type="node_status">
      <field name="COLOUR">#0000FF</field>
    </block>    
    <block type="node_log"></block>
    <block type="node_clone">
      <value name="OBJECT_INPUT">
        <shadow type="node_msg"></shadow>
      </value> 
    </block>
    <block type="node_properties"></block>
    <block type="node_close"></block>
  </category>
</xml>
